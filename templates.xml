					<Size x="0" y="10"/>
					<Anchors>
						<Anchor point="BOTTOMLEFT" x="10" y="-10"/>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
		<Frames>
			<CheckButton parentKey="enable" inherits="MJCheckButtonTemplate">
				<Anchors>
					<Anchor point="BOTTOMLEFT" relativePoint="TOPLEFT" x="10" y="-3"/>
				</Anchors>
				<Scripts>
					<OnLoad>self.Text:SetText(ENABLE)</OnLoad>
				</Scripts>
			</CheckButton>
			<ScrollFrame parentKey="scrollFrame" inherits="UIPanelScrollFrameTemplate">
				<Anchors>
					<Anchor point="TOPLEFT" x="5" y="-5"/>
					<Anchor point="BOTTOMRIGHT" x="-28" y="5"/>
				</Anchors>
				<ScrollChild>
					<EditBox parentKey="editBox" multiLine="true" letters="255" autoFocus="false" countInvisibleLetters="true">
						<Size x="359"/>
						<Scripts>
							<OnLoad>
								ScrollingEdit_OnCursorChanged(self, 0, 0, 0, 0);
							</OnLoad>
							<OnTextChanged>
								self:GetParent():GetParent().limitText:SetFormattedText(MACROFRAME_CHAR_LIMIT, self:GetNumLetters())
								ScrollingEdit_OnTextChanged(self, self:GetParent());
							</OnTextChanged>
							<OnCursorChanged function="ScrollingEdit_OnCursorChanged"/>
							<OnUpdate>
								ScrollingEdit_OnUpdate(self, elapsed, self:GetParent());
							</OnUpdate>
							<OnEscapePressed function="EditBox_ClearFocus"/>
						</Scripts>
						<FontString inherits="GameFontHighlightSmall"/>
					</EditBox>
				</ScrollChild>
			</ScrollFrame>
			<Button>
				<Anchors>
					<Anchor point="TOPLEFT" x="5" y="-5"/>
					<Anchor point="BOTTOMRIGHT" x="-28" y="5"/>
				</Anchors>
				<Scripts>
					<OnClick>
						self:GetParent().scrollFrame.editBox:SetFocus()
					</OnClick>
				</Scripts>
			</Button>
			<Button parentKey="defaultBtn" inherits="UIPanelButtonTemplate">
				<Size x="105" y="22"/>
				<Anchors>
					<Anchor point="BOTTOMRIGHT" relativePoint="TOPRIGHT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnLoad>self:SetText(DEFAULT)</OnLoad>
					<OnClick>PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);</OnClick>
				</Scripts>
			</Button>
			<Button parentKey="cancelBtn" inherits="UIPanelButtonTemplate">
				<Size x="80" y="22"/>
				<Anchors>
					<Anchor point="TOPRIGHT" relativePoint="BOTTOMRIGHT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnLoad>self:SetText(CANCEL)</OnLoad>
					<OnClick>PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);</OnClick>
				</Scripts>
			</Button>
			<Button parentKey="saveBtn" inherits="UIPanelButtonTemplate">
				<Size x="80" y="22"/>
				<Anchors>
					<Anchor point="RIGHT" relativeKey="$parent.cancelBtn" relativePoint="LEFT" x="0" y="0"/>
				</Anchors>
				<Scripts>
					<OnLoad>self:SetText(SAVE)</OnLoad>
					<OnClick>PlaySound(SOUNDKIT.IG_MAINMENU_OPTION_CHECKBOX_ON);</OnClick>
				</Scripts>
			</Button>
		</Frames>
		<Scripts>
			<OnLoad>
				self:SetBackdropBorderColor(TOOLTIP_DEFAULT_COLOR:GetRGB());
				self:SetBackdropColor(TOOLTIP_DEFAULT_BACKGROUND_COLOR:GetRGB());
			</OnLoad>
		</Scripts>
	</Frame>

